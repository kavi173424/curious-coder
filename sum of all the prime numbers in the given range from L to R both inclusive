// Online C++ compiler to run C++ program online
#include <iostream>
#include <math.h>
using namespace std;

int main() {
 int n=20;
 int a[20]={0};
for(int i=2;i<=sqrt(n);i++){
    if(a[i]==0){
    for(int j=2*i;j<=n;j=j+i){
        a[j]=1;
        
    }
    }
}
int sum = 0;
for(int i=10;i<=n;i++){
    if(a[i]==0){
        
        sum+=i;
       
    }
}
 cout<<sum;
    return 0;
}
