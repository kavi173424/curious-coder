#include <iostream>
using namespace std;
int main() {
int arr1[] = {5, 7, 9, 3, 6, 2};
int arr2[] = {1, 2, 6, 1, 9};
int n=6;
int m=5;
int max1=arr1[0];
int min2=arr2[0];
for(int i=1;i<n;i++){
    if(arr1[i]>max1){
        max1=arr1[i];
    }
}
for(int i=1;i<m;i++){
    if(arr2[i]<min2){
        min2=arr2[i];
    }
}

cout<<min2*max1;
    return 0;
}
